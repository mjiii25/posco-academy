    "t = int(input())\n",
    "\n",
    "answer = 0\n",
    "answers = []\n",
    "\n",
    "for i in range(t) :\n",
    "    a, *b = map(int, input().split())\n",
    "    answer += a\n",
    "    \n",
    "    for i in b :\n",
    "        answer += i\n",
    "        \n",
    "    answers.append(answer)\n",
    "    answer = 0\n",
    "\n",
    "\n",
    "for answer in answers :\n",
    "    print(answer)\n",
    "    "
   ]
  }
