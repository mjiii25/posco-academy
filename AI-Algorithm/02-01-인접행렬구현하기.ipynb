    "t = int(input())\n",
    "\n",
    "for _ in range(t) :\n",
    "    N, M = map(int, input().split())\n",
    "    mat = [[0]*N for _ in range(N)]\n",
    "    \n",
    "    for _ in range(M) :\n",
    "        u, v, c = map(int, input().split())        \n",
    "        mat[u][v] = c\n",
    "    \n",
    "    for i in range(N) :\n",
    "        print(*mat[i])\n",
    "        "
